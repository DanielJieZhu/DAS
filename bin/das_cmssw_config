#!/bin/sh

if  [ -z $DAS_PYTHONROOT ]; then
   echo "DAS_PYTHONROOT environment is not set"
   exit 1
fi

dir=/mnt/disk1/cmssw/
dir=/afs/cern.ch/cms/sw
export SCRAM_ARCH=slc5_amd64_gcc434
dir=/Users/vk/CMS/CMSSW
export SCRAM_ARCH=slc5_ia32_gcc434

# run actual script
cmd=`ls $dir/$SCRAM_ARCH/cms/cmssw/ | egrep '^CMSSW_*' | sed "s,/,,g"`
for release in $cmd
do
    echo "Trying $release ..."
    echo "$DAS_PYTHONROOT/DAS/tools/cmssw_config_db.py --path=$dir --release=$release"
    $DAS_PYTHONROOT/DAS/tools/cmssw_config_db.py --path=$dir --release=$release
done

